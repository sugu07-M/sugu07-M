 joinData() {
    // Assuming 'supportId' is the common key
    this.supportTeamData.forEach(supportTeam => {
      const matchingTeamAssigned = this.teamAssignedData.find(teamAssigned => teamAssigned.supportId === supportTeam.supportId);
      if (matchingTeamAssigned) {
        supportTeam.teamAssigned = matchingTeamAssigned;
      }
    });
  }
 <tbody>
    <tr *ngFor="let supportTeam of supportTeamData" class="tr-body">
      <td>{{ supportTeam.supportId }}</td>
      <td>{{ supportTeam.firstName + ' ' + supportTeam.lastName }}</td>
      <td>{{ supportTeam.plantId }}</td>
      <td>
        <ng-container *ngIf="supportTeam.teamAssigned?.isChangeAnalyst; else notChangeAnalyst">
          <i class="fas fa-check"></i>
        </ng-container>
        <ng-template #notChangeAnalyst>
          <i class="fas fa-times"></i>
        </ng-template>
      </td>
      <td>
        <ng-container *ngIf="supportTeam.teamAssigned?.isSupportEngineer; else notSupportEngineer">
          <i class="fas fa-check"></i>
        </ng-container>
        <ng-template #notSupportEngineer>
          <i class="fas fa-times"></i>
        </ng-template>
      </td>
      <td>
        <ng-container *ngIf="supportTeam.teamAssigned?.isApprover; else notApprover">
          <i class="fas fa-check"></i>
        </ng-container>
        <ng-template #notApprover>
          <i class="fas fa-times"></i>
        </ng-template>
      </td>
      <td>
        <a href="" [routerLink]="'/support'">
          <i class="fa fa-eye" style="font-size:15px;color:black"></i>
        </a>
      </td>
    </tr>
  </tbody>
