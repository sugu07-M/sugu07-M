{
        // Filter checklist based on conditions
        this.checklist = response.filter(item =>
          item.supportID === this.supportID &&
          item.categoryID === this.categoryID &&
          item.classificationID === this.classificationID
        );
      },
 checklist: any[] = [];
  supportID: number = 0; // Assign the appropriate supportID
  categoryID: number = 0; // Assign the appropriate categoryID
  classificationID: number = 0;
