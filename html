 generateChartLabels(monthData: number[]): string[] {
    const labels: string[] = [];
    const today: Date = new Date();
    let currentMonth: number = today.getMonth();
    let currentYear: number = today.getFullYear();

    for (let i: number = 6; i >= 0; i--) {
      const monthIndex = (currentMonth - i + 12) % 12;
      const month: string = this.getMonthName(monthData[monthIndex]);
      const year: number = currentYear - Math.floor((i - 1) / 12);

      labels.push(`${month} ${year}`);
    }
    return labels; 
  }

  getMonthName(monthIndex: number): string {
    // You can implement logic to get month name from month index
    // For simplicity, let's assume month index corresponds to the month name
    const monthNames = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];
    return monthNames[monthIndex];
  }
