<div *ngIf="monthData">
  <h2>{{ monthData.label }}</h2>
  <p>Completed: {{ monthData.completed }}</p>
  <p>Open: {{ monthData.open }}</p>
</div>

  monthData: any;

  constructor(private router: Router) {
    const navigation = this.router.getCurrentNavigation();
    if (navigation && navigation.extras && navigation.extras.state) {
      this.monthData = navigation.extras.state;
    }
  }

  ngOnInit(): void {
    console.log(this.monthData); // Log the received data
  }
