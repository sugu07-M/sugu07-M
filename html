import { Routes, RouterModule } from '@angular/router';
RouterModule.forRoot(routes)

<div *ngFor="let item of data">
  <!-- Display data for the selected month -->
  <p>{{ item | json }}</p> <!-- Adjust this according to your data structure -->
</div>

import { Component, OnInit } from '@angular/core';
import { ActivatedRoute } from '@angular/router';
import { DataService } from 'path-to-your-data-service'; // Adjust this import as per your project structure

@Component({
  selector: 'app-detail-page',
  templateUrl: './detail-page.component.html',
  styleUrls: ['./detail-page.component.css']
})
export class DetailPageComponent implements OnInit {
  month: string;
  data: any[];

  constructor(
    private route: ActivatedRoute,
    private dataService: DataService // Inject your data service
  ) { }

  ngOnInit(): void {
    this.route.params.subscribe(params => {
      this.month = params['month'];
      this.loadData();
    });
  }

  loadData() {
    // Assuming you have a method in your data service to fetch data for the specified month
    this.dataService.getDataForMonth(this.month).subscribe((data: any[]) => {
      this.data = data;
    });
  }
}
