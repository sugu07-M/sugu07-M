getchangerequest() {

   const apiUrls = this.apiurl + '/ViewChangeRequest/ViewChangerequest';
   const requestBody = {}

   const httpOptions = {
     headers: new HttpHeaders({
       'content-Type': 'application/json'
     })
   };

   this.http.get(apiUrls, requestBody).subscribe(
     (response: any) => {
       this.changerequest = response;
       this.countStatus();

     },

     (error) => {
       console.error("Post failed", error);
     }
   );
 }
  this.release = this.changerequest.filter(item => item.status.trim() === 'Release')
  this.closure = this.changerequest.filter(item => item.status.trim() === 'Closure')
