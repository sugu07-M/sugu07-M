 onChartClick(event: any) {
    const labelClicked = this.barChartLabels[event.active[0]._index];
    if (labelClicked === 'Open') {
      this.router.navigate(['/open-page']); // Navigate to 'open-page' component
    } else if (labelClicked === 'Completed') {
      this.router.navigate(['/completed-page']); // Navigate to 'completed-page' component
    }
  }
