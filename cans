selectedPlants: any[] = [];

getplant() {
  const apiUrls = this.apiurl + '/Plantid';
  const httpOptions = {
    headers: new HttpHeaders({
      'Content-Type': 'application/json'
    })
  };

  this.http.get(apiUrls, httpOptions).subscribe(
    (response: any) => {
      console.log(response);
      this.plantcode = response;
      console.log(this.plantcode);
    },
    (error) => {
      console.error('Post failed', error);
    }
  );
}

 <div class="col card_plant_filter">
  <ng-select [multiple]="true" [(ngModel)]="selectedPlants" [ngModelOptions]="{standalone: true}" class="input-change">
    <ng-option *ngFor="let plant of plantcode" [value]="plant.id">{{ plant.code }}</ng-option>
  </ng-select>
</div>
