 chartClicked(event: ChartEvent) {
    if (event.active && event.active.length > 0) {
      const label = event.active[0]._model.label;
      switch (label) {
        case 'Label 1':
          this.router.navigate(['/filterdashboard']);
          break;
        case 'Label 2':
          this.router.navigate(['/filterdashboard']);
          break;
        default:
          console.log('No action defined for this label');
      }
    }
  }
