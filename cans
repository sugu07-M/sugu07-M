Error	TS2532	(TS) Object is possibly 'undefined'.	D:\IT_Portal\IT-Portal\IT-Portal.UI\tsconfig.json	D:\IT_Portal\IT-Portal\IT-Portal.UI\src\app\dashboard\dashboard.component.ts	170	Active


public chartClicked(event: any): void {
  if (event.active && event.active.length > 0 && this.doughnutChartData.labels) {
    const clickedLabel = this.doughnutChartData.labels[event.active[0]._index];
    if (clickedLabel) {
      this.navigateToPage(clickedLabel);
    }
  }
}

  private navigateToPage(label: string): void {
    switch (label) {
      case 'Draft':
        this.router.navigate(['/draft']);
        break;
      case 'Pending Approval':
        this.router.navigate(['/pending-approval']);
        break;
      case 'Approved':
        this.router.navigate(['/approved']);
        break;
      case 'Rejected':
        this.router.navigate(['/rejected']);
        break;
      case 'Implementation':
        this.router.navigate(['/implementation']);
        break;
      case 'Pending':
        this.router.navigate(['/pending']);
        break;
      case 'Release':
        this.router.navigate(['/release']);
        break;
      case 'Closure':
        this.router.navigate(['/closure']);
        break;
      default:
        console.warn('Unhandled label:', label);
        break;
    }
  }
