(ngModelChange)="filterData()"
filterData() {
  // If searchText is empty, show all data
  if (!this.searchText.trim()) {
    this.filteredData = this.supportAssignName;
    return;
  }

  // Filter based on searchText
  this.filteredData = this.supportAssignName.filter(item =>
    item.firstName.toUpperCase().includes(this.searchText.trim().toUpperCase())
  );
}
