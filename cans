  chartClicked(event: any, chart: Chart) {
    // Check if the event object exists
    if (event) {
      // Access the active elements through the event object
      const activeElements = chart.getElementsAtEventForMode(event, 'point', chart.options)[0];
      if (activeElements) {
        // Access the label from the active elements
        const label = activeElements._model.label;
        // Navigate based on the label
        switch (label) {
          case 'Open':
            this.router.navigate(['/filterdashboard']);
            break;
          case 'Completed':
            this.router.navigate(['/filterdashboard']);
            break;
          default:
            console.log('No action defined for this label');
        }
      }
    }
  }
