
  filterChangeRequest() {
    this.Draft = this.Draft.filter(item => item.status.trim() === 'Draft');
  }

  // Function to filter data based on status and plant ID
  filterDraftView(status: string, plantId: string) {
    this.filteredDraftData = this.Draft.filter(item => item.status.trim() === status && item.plantId === plantId);
  }


 navigatedraft() {
   this.router.navigate(['/filterdraft'])
 }
