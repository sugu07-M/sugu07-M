   <thead class="table-head">
     <tr class="border-bottom">
       <th>Priority ID</th>
       <th>Priority Name</th>
       <th>Is Active</th>
       <th>Action</th>
     </tr>
   </thead>
   <tbody>
     <tr  class="tr-body">
       <td></td>
       <td></td>
       <td></td>

      
     </tr>
   </tbody>
 </table>
