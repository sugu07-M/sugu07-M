  <div style="width:50%">
    <ng-multiselect-dropdown [settings]="dropdownSettings"
                             [data]="dropdownList">
    </ng-multiselect-dropdown>
  </div>

TS2322: Type '{ item_id: number; item_text: string; }' is not assignable to type 'never'.

 ngOnInit() {
   this.dropdownList = [
     { item_id: 1, item_text: 'Change Request' },
     { item_id: 2, item_text: 'Closure' },
     { item_id: 3, item_text: 'Releaser' }
    
   ];
   this.dropdownSettings = {
     idField: 'item_id',
     textField: 'item_text',
   };
  dropdownList: { item_id: number; item_text: string; }[];
