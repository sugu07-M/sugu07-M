filterData() {
    const searchTerm = this.searchText.trim().toUpperCase(); // Trim whitespace and convert to uppercase
    this.filteredData = this.supportAssignName.filter(item =>
      item.firstName.toUpperCase().includes(searchTerm)
    );
  }

filterData() {
  const searchTerm = this.searchText.trim().toUpperCase(); // Trim whitespace and convert to uppercase
  this.filteredData = this.supportAssignName.filter(item =>
    item.firstName.toUpperCase().includes(searchTerm)
  );
}

