
    onItemSelect(item: any) {
        this.selectedItems.push(item);
    }

    onSelectAll(items: any) {
        this.selectedItems = items;
    }

    onItemDeSelect(item: any) {
        this.selectedItems = this.selectedItems.filter(selectedItem => selectedItem !== item);
    }

    onUnSelectAll() {
        this.selectedItems = [];
    }
