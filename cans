Error	CS1061	'SPEmployeeMaster' does not contain a definition for 'GetAwaiter' and no accessible extension method 'GetAwaiter' accepting a first argument of type 'SPEmployeeMaster' could be found (are you missing a using directive or an assembly reference?)	IT_Portal.API	D:\IT-Portal\IT-Portal\IT_Portal.API\Controllers\EmployeeMastersController.cs	22	Active
    [Route("api/[controller]")]
    [ApiController]
    public class EmployeeMastersController : ControllerBase
    {
        private readonly IEmployeeMasters _masterscontext;

        public EmployeeMastersController(IEmployeeMasters masterscontext)
        {
            this._masterscontext = masterscontext;
        }

        [HttpGet]
        public async Task<IActionResult> GetEmployee()
        {
            var employeedata = await _masterscontext.GetEmployeMaster();
            return Ok(employeedata);
        }
    }
}
