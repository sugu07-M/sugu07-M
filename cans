ChangeDetectionStrategy
 changeDetection: ChangeDetectionStrategy.OnPush
 this.loadData();

  loadData(): void {
    // Fetch data asynchronously
    this.http.get<any>('https://api.example.com/data')
      .subscribe(
        (response) => {
          // Handle response
          this.data = response;
        },
        (error) => {
          // Handle error
          console.error('Error fetching data:', error);
        }
      );
  }
