 Draft: any[] = [];
 getChangeRequest() {
   const apiUrls = this.apiurl + '/ViewChangeRequest/ViewChangerequest';
   this.http.get<any[]>(apiUrls).subscribe(
     (response) => {
       this.Draft = response;
       this.filterChangeRequest();
     },
     (error) => {
       console.error("GET request failed", error);
     }
   );
 }
 filterChangeRequest() {
   
   this.Draft = this.Draft.filter(item => item.status.trim() === 'Draft');
 }


 navigatedraft() {
   this.router.navigate(['/filterdraft'])
 }
