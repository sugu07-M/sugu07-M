  chartClicked(event: ChartEvent) {
    // Check if the event object and its properties exist
    if (event && event.native && event.native.active && event.native.active.length > 0) {
      // Access the label from the event object
      const label = event.native.active[0]._model.label;
      // Navigate based on the label
      switch (label) {
        case 'Label 1':
          this.router.navigate(['/filterdashboard']);
          break;
        case 'Label 2':
          this.router.navigate(['/filterdashboard']);
          break;
        default:
          console.log('No action defined for this label');
      }
    }
  }
