 <label for="startDate">Start Date:</label>
  <label for="plantId">Select Plant ID:</label>
  <label for="endDate">End Date:</label>
 allowSearchFilter: true
  this.http.get(apiUrls, httpOptions).subscribe(
    (response: any) => {
      this.changerequest = response;
      // Update doughnut chart with all data
      this.countStatus();
    },
    (error) => {
      console.error('Fetching all change request data failed', error);
    }
  );
}
