 startDate: string = '';
 endDate: string='';

this.filter = this.changerequest.filter((item: any) => {
   // Check if the item's plantId is in the selectedPlantIds array
   const selectedPlantIds = this.selectedPlantIds.map(item.plantId);
   // Check if the item's date is within the specified range
   const startDate = this.startDate.map(item.crdate);
   const endDate = this.endDate.map(item.crdate);
   // Return true if both conditions are met
   return selectedPlantIds && startDate && endDate ;
 });
TS2339: Property 'map' does not exist on type 'string'.
